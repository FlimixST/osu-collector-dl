{
  "name": "osu-collector-dl",
  "version": "2.6.2",
  "main": "./dist/index.js",
  "scripts": {
    "build": "yarn build-app-win && yarn build-app-linux",
    "start": "yarn build-ts && yarn run start",
    "test": "yarn build-ts && node ./dist/__test/test.js",
    "build-ts": "tsc -p .",
    "build-app-win": "yarn build-ts && pkg -t node16-win-x64 -o ./build/win-x64/osu-collector-dl.exe ./dist/index.js --public",
    "build-app-linux": "yarn build-ts && pkg -t node16-linux -o ./build/linux-arm64/osu-collector-dl ./dist/index.js --public"
  },
  "license": "MIT",
  "dependencies": {
    "csbinary": "^2.1.4",
    "prompt-sync": "^4.2.0",
    "tslib": "^2.4.0",
    "undici": "^5.8.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.6"
  },
  "packageManager": "yarn@3.2.1"
}
